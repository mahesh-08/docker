  product-management-compose:
    image: product-management-compose
    ports:
      - "8080:8080"
    networks:
      - foo
    depends_on:
      - mysqldb
 
  mysqldb:
    image: mysql
    networks:
      - foo
    environment:
      - MYSQL_ROOT_PASSWORD=root
      - MYSQL_DATABASE=fullstack
      - MYSQL_USER=test
      - MYSQL_PASSWORD=root  
    ports:
      - 3308:3306
      
